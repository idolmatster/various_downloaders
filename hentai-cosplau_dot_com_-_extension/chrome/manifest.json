{
    "name": "henimg",
    "version": "1.0",
    "description": "helping with downloading images",
    "manifest_version": 2,
    "permissions": [
        "contextMenus",
        "downloads",
        "clipboardWrite"
    ],
    "background": {
        "scripts": [
            "action.js"
        ]
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    }
}